//61.Opposites Attract
//https://www.codewars.com/kata/555086d53eac039a2a000083/train/javascript
function lovefunc(flower1, flower2){
    console.log(flower1+" "+flower2+" "+flower1%2+" "+!flower2%2);
    if (flower1%2 !== flower2%2){
        return true
    }
    return false
}

