}

//9.Multiplication table for number
//https://www.codewars.com/kata/5a2fd38b55519ed98f0000ce/train/javascript
function multiTable(number) {
    var r = "";
    for (i = 1; i<11; i++){
        if (i < 10){
            r += i+" * "+number+" = "+(number*i)+"\n";
        } else {
            r += i+" * "+number+" = "+(number*i);
        }
    }
    return r;
}

