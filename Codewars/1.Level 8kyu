//35.Invert values
//https://www.codewars.com/kata/5899dc03bc95b1bf1b0000ad/train/javascript
function invert(array) {
    for(i=0;i<array.length;i++){
        if(array[i] >= 0){
            array[i] = -array[i];
        } else if(array[i] < 0){
            array[i] *= -1;
        }
    }
    return array;
}

//36.Count of positives / sum of negatives
//https://www.codewars.com/kata/576bb71bbbcf0951d5000044/train/javascript
function countPositivesSumNegatives(input) {
    console.log(input);
    var tmp = [];
    var po = 0;
    var ne = 0;
    if(input === undefined || input === null || input.length == 0){
        return tmp;
    } else {
        for(i=0;i<input.length;i++){
            if(input[i]>0){
                po += 1;
            } else {
                ne += input[i];
            }
        }
    }
    tmp[0] = po;
    tmp[1] = ne;
    return tmp;
}

//37.Beginner Series #2 Clock
//https://www.codewars.com/kata/55f9bca8ecaa9eac7100004a/train/javascript
function past(h, m, s){
    var sum = 0;
    if (s != 0){
        s *= 1000;
    }
    if (m != 0){
        m *= 60 * 1000;
    }
    if (h != 0){
        h *= 60 * 60 * 1000;
    }
    sum = s + m + h;
    return sum;
}

//38.MakeUpperCase
//https://www.codewars.com/kata/57a0556c7cb1f31ab3000ad7/train/javascript
function makeUpperCase(str) {
    var nv = "";
    for(i=0;i<str.length;i++){
        nv += str[i].toUpperCase();
    }
    return nv;
}

//39.Is he gonna survive?
//https://www.codewars.com/kata/59ca8246d751df55cc00014c/train/javascript
function hero(bullets, dragons){
    var answer = bullets/(dragons*2);
    if(answer >= 1){
        return true;
    }
    return false;
}

//40.Calculate average
//https://www.codewars.com/kata/57a2013acf1fa5bfc4000921/train/javascript
function find_average(array) {
    var len = array.length;
    var num = 0;
    if(array.length == 0){
        return 0;
    }
    for(i=0;i<array.length;i++){
        num += array[i];
    }
    return num/len;
}

