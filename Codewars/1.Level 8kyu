//43.Fake Binary
//https://www.codewars.com/kata/57eae65a4321032ce000002d/train/javascript
function fakeBin(x){
    let newValue = "";
    for(i=0;i<x.length;i++){
        if(x[i] < 5){
            newValue += 0;
        } else {
            newValue += 1;
        }
    }
    return newValue;
}

