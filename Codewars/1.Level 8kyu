//120.Hello, Name or World!
//https://www.codewars.com/kata/57e3f79c9cb119374600046b/train/javascript
function hello(name) {
    if (name===undefined || name===""){
        return "Hello, World!";
    }
    let res = name.toLowerCase();
    let res2 = res.charAt(0).toUpperCase()+ res.slice(1)
    return `Hello, ${res2}!`;
}

//125.Lario and Muigi Pipe Problem
//https://www.codewars.com/kata/56b29582461215098d00000f/train/javascript
function pipeFix(numbers){
    numbers.sort(function(a, b) {
        return a - b;
    });

    let res = [];
    for (let i=numbers[0];i<=numbers[numbers.length-1];i++){
        res.push(i);
    }
    return res;
}

