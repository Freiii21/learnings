}

//153.Find the position!
//https://www.codewars.com/kata/5808e2006b65bff35500008f/train/javascript
function position(letter){
    return `Position of alphabet: ${letter.charCodeAt(0)-96}`
}

//154.Remove duplicates from list
//Remove duplicates from list
function distinct(a) {
    return [...new Set(a)];
}

//155.Alan Partridge II - Apple Turnover
//https://www.codewars.com/kata/580a094553bd9ec5d800007d/train/javascript
function apple(x){
    if (x ** 2 > 1000) {
        return "It's hotter than the sun!!"
    }
    return "Help yourself to a honeycomb Yorkie for the glovebox."
}

//156.Multiple of index
//https://www.codewars.com/kata/5a34b80155519e1a00000009/train/javascript
function multipleOfIndex(array) {
    let resultArr = [];

    for (let i = 0; i < array.length; i++){
        if (array[i] % i === 0){
            resultArr.push(array[i])
        }
    }

    return resultArr;
}

//157.Surface Area and Volume of a Box
//https://www.codewars.com/kata/565f5825379664a26b00007c/train/javascript
function getSize(width, height, depth){
    const area = (width * height * 2) + (width * depth * 2) + (height * depth * 2);
    const volume = width * height * depth;

    return [area, volume];
}

