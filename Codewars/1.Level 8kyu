function arrayPlusArray(arr1, arr2) {
    let sum = 0;
    for (let i=0; i< arr1.length; i++){
        sum += arr1[i];
    }
    for (let i=0; i< arr2.length; i++){
        sum += arr2[i];
    }
    return sum; //something went wrong
}

//47.Are You Playing Banjo?
//https://www.codewars.com/kata/53af2b8861023f1d88000832/train/javascript
function areYouPlayingBanjo(name) {
    let f = name[0];
    if (f === 'R' || f === 'r'){
        return name+" plays banjo";
    }
    return name + " does not play banjo";
}

//48.Sum without highest and lowest number
//https://www.codewars.com/kata/576b93db1129fcf2200001e6/train/javascript
function sumArray(array) {
    if (array === [] || array === null || array.length === 1){
        return 0;
    }
    let sortArray = array.sort(function (a,b){return a-b});
    sortArray.pop();
    sortArray.shift();
    let result = 0;
    for (let i=0; i< sortArray.length; i++){
        result += sortArray[i];
    }
    return result;
}

//49.Total amount of points
//https://www.codewars.com/kata/5bb904724c47249b10000131/train/javascript
function points(games) {
    console.log(games);
    let count = 0;
    for (let i=0; i<games.length; i++){
        let x = games[i][0];
        let y = games[i][2];
        if(x>y){
            count +=3;
        } else if(x==y){
            count +=1;
        }
    }
    console.log(games)
    return count;
}

//50.Reversed sequence
//https://www.codewars.com/kata/5a00e05cc374cb34d100000d/train/javascript
const reverseSeq = n => {
    let array = [];
    for (let i=n; i>0; i--){
        array.push(i)
    }
    return array;
};

//51.Do I get a bonus?
//https://www.codewars.com/kata/56f6ad906b88de513f000d96/train/javascript
function bonusTime(salary, bonus) {
    if (bonus){
        salary *= 10;
    }
    return "\u00A3"+salary;
}

//52.Beginner - Reduce but Grow
//https://www.codewars.com/kata/57f780909f7e8e3183000078/train/javascript
function grow(x){
    let mult = 1;
    for (let i=0; i<x.length; i++){
        mult *= x[i];
    }
    return mult;
}

//53.Beginner Series #1 School Paperwork
//https://www.codewars.com/kata/55f9b48403f6b87a7c0000bd/train/javascript
function paperwork(n, m) {
    if (n <= 0 || m <= 0){
        return 0;
    }
    return n*m;
}

//54.Find the first non-consecutive number
//https://www.codewars.com/kata/58f8a3a27a5c28d92e000144/train/javascript
function firstNonConsecutive (arr) {
    console.log(arr);
    if (arr === [] || arr.length === 1){
        return null;
    }
    for(let i=0; i<arr.length-1; i++){
        if (arr[i+1] - arr[i] !== 1){
            return arr[i+1]
        }
    }
    return null;
}

//55.Find Maximum and Minimum Values of a List
//https://www.codewars.com/kata/577a98a6ae28071780000989/train/javascript
var min = function(list){
    let mi = list[0];
    for (let i=1; i < list.length; i++){
        if(mi > list[i]){
            mi = list[i];
        }
    }
    return mi;
}

var max = function(list){
    let ma = list[0];
    for (let i=1; i < list.length; i++){
        if(ma < list[i]){
            ma = list[i];
        }
    }
    return ma;
}

//56.DNA to RNA Conversion
//https://www.codewars.com/kata/5556282156230d0e5e000089/train/javascript
function DNAtoRNA(dna) {
    let result = "";
    for (let i=0; i<dna.length; i++){
        if(dna[i] === "T"){
            result += "U";
        } else {
            result += dna[i];
        }
    }
    return result;
}

//57.How good are you really?
//https://www.codewars.com/kata/5601409514fc93442500010b/train/javascript
function betterThanAverage(classPoints, yourPoints) {
    let av = 0;
    classPoints.push(yourPoints);
    for(let i=0; i<classPoints.length;i++){
        av += classPoints[i];
    }
    av /= classPoints.length;
    if (av >= yourPoints){
        return false;
    }
    return true;
}

//58.Rock Paper Scissors!
//https://www.codewars.com/kata/5672a98bdbdd995fad00000f/train/javascript
const rps = (p1, p2) => {
    if (p1 === p2){
        return 'Draw!'
    } else if ((p1 === "rock" && p2 === 'scissors') || (p1 === "scissors" && p2 === 'paper') || (p1 === "paper" && p2 === 'rock')){
        return 'Player 1 won!'
    }
    return 'Player 2 won!'
};

//59.Count by X
//https://www.codewars.com/kata/5513795bd3fafb56c200049e/train/javascript
function countBy(x, n) {
    let z = [];
    for(let i = 1; i<n+1; i++){
        z.push(x*i);
    }
    return z;
}

