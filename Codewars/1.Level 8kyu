//141.SpeedCode #2 - Array Madness
//https://www.codewars.com/kata/56ff6a70e1a63ccdfa0001b1/train/javascript
function arrayMadness(a, b) {
    const result1 = a.reduce((total, element)=>{return total + element**2}, 0)
    const result2 = b.reduce((total, element)=>{return total + element**3}, 0)

    return result1 > result2;
}

